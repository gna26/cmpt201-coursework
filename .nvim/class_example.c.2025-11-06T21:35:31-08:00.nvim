#include <pthread.h>
#include <stdio.h>
#include <string.h>

void *func(void *string) {
  char *s = (char *)string;
  printf("inside thread");
  return (void *)strlen(s);
}

int main() {
  pthread_t thread1;
  pthread_create(&thread1, NULL, func, "string");

  void *retval;
  pthread_join(thread1, &retval);

  long long *ans = (long long *)retval;
  printf("%d", ans);
}
